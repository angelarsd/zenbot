<context>
  <input pattern="(Empezar|Apreta Comenzar) *">
    <output value="Indicanos tu DNI para que te podamos indicar la informacion correspondiente"/>
    <input pattern="$Text">
        <var name="DNI" value="$dni" scope="dni"/>
        <output value="Tu DNI es $DNI!"/>
      </input>
  </input>
</context>
